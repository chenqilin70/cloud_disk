html , body , #wrapper{
	width: 100%;
	height: 100%;
}
#page-wrapper {
	height: 100%;
    min-height: 500px;
    position: fixed;
}
.box{
	width: 80%;
    height: 90%;
    padding-top: 20px;
 min-width: 940px;
}
.leftBox
{
	width: 30%;
	display:inline-block;
	height: 100%;
}
.rightBox{
	width: 68%;
	display:inline-block;
	background-color: #ffffff;
	vertical-align: top;
	height: 100%;
}
.sessionUl , .friendUl{
	margin:0;
	padding:0;
	list-style: none;
	border-radius:5px;
}
.sessionUl  li , .friendUl  li{
	height: 70px;
	width: 100%;
}
.tabContent{
	height: 100%;
	background-color: #fff;
	border-right: 1px solid  #CCCCCC;
	border-left: 1px solid  #CCCCCC;
	overflow: auto;
	
}
.tabContentTd{
	background-color: #ffffff;
	position: relative;
}
.sessionInfo  , .friendInfo{
	height: 100%;
	width: 100%;
	position: relative;
}
.sessionHead , .friendHead ,.friendHeadInPane{
	height: 60px;
	width: 60px;
	display:inline-block;
	margin-top:5px;
	margin-left: 5px;
}

.sessionCenterInfo{
	display:inline-block;
	vertical-align: top;
	margin-top:5px;
	line-height: 30px;
	
}
.friendCenterInfo{
	display:inline-block;
	vertical-align: top;
	margin-top:5px;
	line-height: 65px;
	margin-left: 8px;
}
.sessionTime{
	position:absolute;
	margin-top:5px;
	display:inline-block;
	font-size:12px;
	color:#999999;
	margin-right: 5px;
	right: 0;
}
.latelyRecord{
	color:#999999;
}
.leftTable{
	table-layout: fixed;
	height: 100%;
}
.leftTable td{
	padding: 0;
	margin: 0;
}
.navTd{
	height: 42px;
}
.footTd{
	height: 34px;
	background-color: #ffffff
}
.footDiv{
	height: 100%;
	width: 100%;
}
#friends{
	display:none;
}
.friendMainTable , .chatTable ,.rightIndexTable{
	height: 100%;
	width: 100%;
	table-layout: fixed;
}
.friendMainCenterTd , .rightIndexCenterTd{
	height: 500px;
}
.rightIndexCenterTd{
	background-image:url('../imgs/right_index.png');
	background-position: center;
	background-repeat: no-repeat;
	position: relative;
}
.friendMainCenterDiv{
	height:100%;
	width:100%;  
}
.friendMainHead{
	height:110px;
	width: 110px; 
}
.friendMainCenterDiv hr{
	margin-left: 20px;
	margin-right: 20px;
}
.friendMainCenterDiv .friendAttr{
	font-size: 16px;
	letter-spacing: 5px;
	line-height: 30px;
}
.palceholder{
	visibility: hidden;
}
.friendMainBtn{
	margin-top: 20px;
	width: 40%;
	display:block;
}
.custombtn{
	background-color: #F36A5A;
	border-color:#F36A5A;
	color:#ffffff;
	padding-top: 6px;
	padding-bottom: 6px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
}
.sessionLi , .friendLi{
	cursor:pointer;
}
.chatWinHead{
	height: 42px;
	background-color: #F5F5F5;
	font-size: 18px;
}
.chatWinFoot{
	height: 34px;
	background-color: #F5F5F5;
}
.chatWinFoot .shareFileInChat{
	display: inline-block;
	width: 10%;
}
.emojiBox{
	display: inline-block;
	width: 80%;
	vertical-align: -14px;
}
.emojiBox > p{
	padding:0;
	margin:0;
}
.emoji-menu{
	position: absolute;
    top: -228px;
}
.sendMessage{
	display:inline-block;
	width: 7%	
}
.createGroupTable{
	table-layout: fixed;
	width: 700px;
	border-left: 1px solid #E5E5E5;
	border-right: 1px solid #E5E5E5;
}
.createGroupHead td{
	height: 30px;
	font-size: 12px;
	background-color: #FAFAFA
}
.createGroupBody td{
	height: 330px;
}
.createGroupDiv{
	height: 100%;
	width: 100%;
	height: 330px;
}
.icheckbox_square-orange{
	float: right;
	margin-top: 24px;
	margin-right: 3px;
}
.unselectedfriend{
	display: none;
}
.icon-shanchu{
	color: #F34733;
    float: right;
    font-size: 25px;
    margin-top: 17.5px;
    margin-right: 10px;
}
#createGroupModal > .modal-dialog{
	width: 800px;
}
#createGroupModal  .modal-body{
	padding-top: 0;
	padding-bottom: 0
}
.addFirendInputGroup{
	width: 80%;
	margin-bottom: 20px;
}
.copyPersonLinkBox{
	width: 80%;
	background-color: #FBFBFB;
	margin-top: 10px;
	padding-bottom: 15px;
}
.copyTit{
	font-size: 13px;
	line-height: 40px;
}
#readyCopy{
	opacity:0;
	position: absolute;
}
.searchResult{
	width: 80%;
	margin-top: 10px;
	margin-bottom: 30px;
	display: none;
}
.searchResultContent{
	width: 100%;
	background-color: #FBFBFB;
	padding-top: 10px;
	padding-bottom: 10px;
}
.resultHead{
	width: 45px;
	height: 45px;
	display:inline-block;
}
.resultHr{
	margin-top:10px;
	margin-bottom: 10px;
}
.inputSearchKeyTip{
	width: 80%
}
.userTipBox{
	height: 120px;
    width: 300px;
    background-color: #ffffff;
    border: 1px solid #DBE2EC;
    position: absolute;
    top:0;
    left:0;
    display: none;
    z-index:500;
    opacity:0;
}
.triangle{
	width:32px;
	height:32px;
    position: absolute;
    top: 44px;
    left: -22px;
}
.userHeadInTip{
	width: 70px;
	height: 70px;
	display:inline-block;
	margin:5px;
}
.infoInTip{
	height: 70px;
	display:inline-block;
	vertical-align: top;
	margin-top: 5px;
	width: 200px;
}
.remarkInTip{
	font-size: 16px;
	font-weight: 600;
	color:rgb(102, 102, 102);
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	
}
.nicknameInTip , .accountInTip{
	font-size: 12px;
	color:#666;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.icon-combinedshape{
	margin-left: 10px;
	cursor: pointer
}
.btnsInTip{
	padding-right: 10px;
}
.btnsInTip button{
	font-size: 12px;
	padding:4px 10px;
}
.remarkInput{
	width: 100%;
}
#addRemarkModal .modal-body{
	padding-left: 40px;
	padding-right: 40px;
}

.noResult{
	color:red;
}

.tips{
	background-color: #F9EDBE;
	position: fixed;
	top:75px;
	left:50%;
	color:#6E6548;
	border-radius:3px;
	padding-left:15px;
	padding-right:15px;
	line-height: 30px;
	border:1px solid #DADB8D;
	display:none;
	opacity:0;
	z-index: 5000
}
.loading{
	display: none;
	margin-bottom: 10px;
}
.indexShareBtn{
	position: absolute;
    bottom: 10%;
    margin-left: -56px;
    left: 50%;
}

























