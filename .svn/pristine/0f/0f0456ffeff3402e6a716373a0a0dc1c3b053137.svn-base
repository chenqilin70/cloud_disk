<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<table class="friendMainTable" border="0" cellspacing="0" cellspadding="0">
	<tbody>
		<tr><td></td></tr>
		<tr>
			<td  class="friendMainCenterTd">
				<div class="friendMainCenterDiv" align="center">
